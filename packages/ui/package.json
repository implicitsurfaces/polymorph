{
  "name": "ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prod": "vite build --config vite.config.ts && vite preview",
    "prof": "vite build --config vite.config.profiling.ts && vite preview",
    "prod-nm": "vite build --config vite.config.nonmin.ts && vite preview",
    "prof-nm": "vite build --config vite.config.profiling-nonmin.ts && vite preview",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "tsc -b",
    "check": "(eslint --max-warnings 0 .; l=$?; tsc -b && exit $l)",
    "preview": "vite preview",
    "format": "prettier --write \"src/*.{jsx,js,tsx,ts}\"",
    "postinstall": "cd ../.. && patch-package"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "comlink": "^4.4.2",
    "fidget": "*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.1.7",
    "short-uuid": "^5.2.0",
    "threejs-math": "^0.147.0",
    "uuid": "^11.0.3"
  },
  "peerDependencies": {
    "draw-api": "*",
    "sketch": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  }
}
