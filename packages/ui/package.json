{
  "name": "ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prod": "vite build --config vite.config.ts && vite preview",
    "prof": "vite build --config vite.config.profiling.ts && vite preview",
    "prod-nm": "vite build --config vite.config.nonmin.ts && vite preview",
    "prof-nm": "vite build --config vite.config.profiling-nonmin.ts && vite preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/*.{jsx,js,tsx,ts}\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.1.7",
    "threejs-math": "^0.147.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  }
}
